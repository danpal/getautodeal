%h1 Listing users
%table
  %tr
    %th Name
    %th email
    %th Make
    %th Model
    %th
    %th
  - @users.each do |user|
    %tr
      %td= user.name
      %td= user.email
      %td= user.make
      %td= user.model
      %td= link_to 'Show', user
      %td= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete

